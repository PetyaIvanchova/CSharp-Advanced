using System;

namespace ex1
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            try
            {
               
                if (n<0)
                {
                    throw new Exception();
                }
                Console.WriteLine(Math.Sqrt(n));
                
            }
            catch(Exception)
            {

                Console.WriteLine("Invalid number.");
            }
            finally
            {
                Console.WriteLine("Goodbye.");
            }


        }
    }
}
