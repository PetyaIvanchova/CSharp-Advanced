using System;
using System.Collections.Generic;
using System.Linq;

namespace ex5
{
    class Program
    {
        static void Main(string[] args)
        {
            Queue<int> opaaa = new Queue<int>();
            int[] numbers = Console.ReadLine().Split().Select(int.Parse).ToArray();

            foreach (var item in numbers)
            {
                if (item%2==0)
                {
                    opaaa.Enqueue(item);
                }
            }
            opaaa.ToArray();
            Console.WriteLine(string.Join(", ",opaaa));
        }
    }
}
